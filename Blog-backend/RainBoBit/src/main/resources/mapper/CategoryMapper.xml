<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wybbb.rainbobit.mapper.CategoryMapper">

    <select id="relateToArticle" resultType="java.lang.Integer">
        SELECT COUNT(*)
        FROM rbb_article
        WHERE category_id = #{categoryId}
    </select>
</mapper>
